{
  "name": "react-starter-kit-ts",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "compile-styles": "sass -s compressed src\\assests\\main.sass:dist\\main.min.css",
    "dev-compile-styles": "sass -s compressed src\\assets\\main.sass:main.min.css",
    "dev-compile": "npm run dev-compile-styles && npm run lint && npm run dev",
    "preview-compile": "npm run compiles-styles && npm run lint && npm run preview",
    "dev-watch-compile-styles": "sass -w -s compressed src\\assests\\main.sass:main.min.css"
  },
  "dependencies": {
    "axios": "^1.5.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.16.0",
    "recoil": "^0.7.7",
    "sass": "^1.69.3"
  },
  "devDependencies": {
    "@types/react": "^18.2.28",
    "@types/react-dom": "^18.2.13",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "@vitejs/plugin-react-swc": "^3.4.0",
    "eslint": "^8.51.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "typescript": "^5.2.2",
    "vite": "^4.4.11"
  }
}
